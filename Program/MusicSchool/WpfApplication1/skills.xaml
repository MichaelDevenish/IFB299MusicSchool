<Window x:Class="WpfApplication1.skills"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApplication1"
        mc:Ignorable="d"
        Title="Skills Management" Height="300" Width="300">
    <Grid>
        <StackPanel HorizontalAlignment="Left" Height="249" Margin="10,10,0,0" VerticalAlignment="Top" Width="272">
            <DataGrid x:Name="dataGrid" Height="186">
                <DataGrid.Columns>
                    <DataGridTextColumn x:Name="firstname" Binding="{x:Null}" ClipboardContentBinding="{x:Null}" Header="First Name"/>
                    <DataGridTextColumn x:Name="lastname" Binding="{x:Null}" ClipboardContentBinding="{x:Null}" Header="Last Name"/>
                    <DataGridTextColumn x:Name="inst_skills" Binding="{x:Null}" ClipboardContentBinding="{x:Null}" Header="Skill"/>
                </DataGrid.Columns>
            </DataGrid>
        </StackPanel>
    </Grid>
</Window>
